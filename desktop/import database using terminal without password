Work only for xampp

/opt/lampp/bin/mysql>mysql -uroot -p DBNAME < PATH_TO_DB 
/opt/lampp/bin/mysql>mysql -uroot -p puur < /home/ntf-22/Downloads/puuroffi.sql 
/opt/lampp/bin/mysql>mysql -uroot -p puuroffi_be < /home/ntf-22/Desktop/puuroffi_be.sql 
/opt/lampp/bin/mysql>mysql -uroot -p test < /home/ntf-36/Desktop/nuancede_wp.sql 


mysql -u root -p batabases name < /databases path
mysql -u root -p ntf@4844 puuroffi_be < /home/ntf-22/Downloads/puuroffice.sql 
